package entities;

public class Aluno {
	private String name;
	private double firstTri;
	private double secondTri;
	private double thirdTri;

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public double getFirstTri() {
		return firstTri;
	}

	public void setFirstTri(double firstTri) {
		this.firstTri = firstTri;
	}

	public double getSecondTri() {
		return secondTri;
	}

	public void setSecondTri(double secondTri) {
		this.secondTri = secondTri;
	}

	public double getThirdTri() {
		return thirdTri;
	}

	public void setThirdTri(double thirdTri) {
		this.thirdTri = thirdTri;
	}

}
