package entities;

public class Account {
	private int id;
	private String holder;
	private double balance;
	
	public Account(int id, String holder) {
		this.id = id;
		this.holder = holder;
	}
	
	public int getId() {
		return id;
	}
	
	public String getHolder() {
		return holder;
	}
	
	public void setHolder(String holder) {
		this.holder = holder;
	}
	
	public double getBalance() {
		return balance;
	}
}
